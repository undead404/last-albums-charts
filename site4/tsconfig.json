{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "esModuleInterop": true,
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "traceResolution": true,
    "typeRoots": ["./types", "./node_modules/@types"]
  }
}
